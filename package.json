{
  "name": "ai-sdk-cost",
  "version": "0.3.3",
  "description": "DB-agnostic cost telemetry helper for Vercel AI SDK with OpenRouter pricing watcher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/R0bk/ai-sdk-cost.git"
  },
  "bugs": {
    "url": "https://github.com/R0bk/ai-sdk-cost/issues"
  },
  "homepage": "https://github.com/R0bk/ai-sdk-cost#readme",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external @opentelemetry/api --external @opentelemetry/core --external @opentelemetry/sdk-trace-base --external @opentelemetry/sdk-trace-node",
    "dev": "tsx watch src/index.ts",
    "fetch:prices": "tsx scripts/fetch-prices.ts",
    "example": "tsx examples/basic.ts",
    "test:external:openai": "tsx examples/openai-example.ts",
    "test:external:anthropic": "tsx examples/anthropic-example.ts",
    "test:anthropic-cache": "tsx examples/anthropic-cache-issue.ts",
    "test:external:google": "tsx examples/google-example.ts",
    "test:external:xai": "tsx examples/xai-example.ts",
    "test:external:mistral": "tsx examples/mistral-example.ts",
    "test:user-override": "tsx examples/user-workspace-override.ts",
    "test:callback": "tsx examples/user-workspace-callback-test.ts",
    "test:pricing": "tsx scripts/verify-pricing.ts"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/core": "^1.30.1",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/google": "2.0.14",
    "@ai-sdk/mistral": "^2.0.0",
    "@ai-sdk/openai": "^2.0.32",
    "@ai-sdk/xai": "^2.0.0",
    "@anthropic-ai/sdk": "^0.35.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "ai": "^5.0.47",
    "express": "^4.19.2",
    "openai": "^4.56.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.12.0"
}
